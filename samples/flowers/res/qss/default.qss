QSlider{
	border-radius:10px;
}
QSlider::add-page:Horizontal{
	background-color: rgb(87, 97, 106);
	height:10px;
	border-radius:10px;
	border-top-left-radius:0px;
	border-bottom-left-radius:0px;
}
QSlider::sub-page:Horizontal{
	background-color: rgb(87, 97, 106);
	height:10px;
	border-radius:10px;
	border-top-right-radius:0px;
	border-bottom-right-radius:0px;
}
QSlider#H{
	background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(255, 0, 0, 255),stop:0.15 rgba(255, 255, 0, 255),stop:0.33 rgba(0, 255, 0, 255),stop:0.49 rgba(1, 255, 255, 255),stop:0.67 rgba(0, 0, 255, 255),stop:0.84 rgba(255, 0, 255, 255),stop:1 rgba(255, 0, 0, 255));
}
QSlider::add-page:Horizontal#H, QSlider::sub-page:Horizontal#H{
	background-color:transparent;
	height:10px;
	border-radius:10px;
	border-top-left-radius:0px;
	border-bottom-left-radius:0px;
}
QSlider::sub-page:Horizontal#S{
	background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(180,220,255,255), stop:1 rgba(50,150,224,255));
	height:10px;
	border-radius:10px;
	border-top-right-radius:0px;
	border-bottom-right-radius:0px;
}
QSlider::sub-page:Horizontal#Value{
	background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(0,0,0,255), stop:1 rgba(220,220,220,220));
	height:10px;
	border-radius:10px;
	border-top-right-radius:0px;
	border-bottom-right-radius:0px;
}
QSlider::sub-page:Horizontal#Intensity{
	background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(0,0,0,255), stop:1 rgba(220,220,220,220));
	height:10px;
	border-radius:10px;
	border-top-right-radius:0px;
	border-bottom-right-radius:0px;
}
QSlider::groove:Horizontal{
	background:transparent;
	height:20px;
	border-radius:10px;
}
QSlider::handle:Horizontal{
	height:20px;
	width:20px;
	background:transparent;
	border-radius:10px;
	border-image: url(:/res/icons/handle.png);
}
